{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "cppcheck-wheel",
  "description": "Package cppcheck as a python wheel.",
  "builtin-baseline": "4b6c50d962cc20aaa3ef457f8ba683b586263cfb",
  "homepage": "https://github.com/msclock/cppcheck-wheel",
  "dependencies": [
    {
      "name": "cppcheck",
      "features": [
        "have-rules"
      ]
    },
    "pcre",
    "cmake-modules"
  ],
  "overrides": [
    {
      "name": "cppcheck",
      "version": "2.16.2"
    },
    {
      "name": "pcre",
      "version": "8.45#7"
    },
    {
      "name": "cmake-modules",
      "version": "1.6.13"
    }
  ],
  "vcpkg-configuration": {
    "registries": [
      {
        "kind": "git",
        "baseline": "e855ff793ab2fa53f26030fab6be443595cfda6e",
        "repository": "https://github.com/msclock/cmake-registry",
        "packages": [
          "cmake-modules",
          "robotology-cmake-ycm",
          "vcpkg-cmake-module"
        ]
      }
    ],
    "overlay-ports": [
      "cmake/vcpkg/ports"
    ],
    "overlay-triplets": [
      "cmake/vcpkg/triplets"
    ]
  }
}
